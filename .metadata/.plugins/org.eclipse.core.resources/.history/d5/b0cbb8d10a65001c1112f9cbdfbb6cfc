package pay.my.buddy.entities;

import java.util.Date;

import javax.persistence.DiscriminatorValue;
import javax.persistence.Entity;

@Entity
@DiscriminatorValue("S")
public class VirementSent extends Operation{
	private Long codeRecipient;
	private String nameRecipient;

	public String getNameRecipient() {
		return nameRecipient;
	}

	public void setNameRecipient(String nameRecipient) {
		this.nameRecipient = nameRecipient;
	}

	public VirementSent() {
		super();
	}

	public Long getCodeRecipient() {
		return codeRecipient;
	}

	public void setCodeRecipient(Long codeRecipient) {
		this.codeRecipient = codeRecipient;
	}

	public VirementSent(Date dateOperation, double amount, String description, Compte compte, Long codeRecipient) {
		super(dateOperation, amount, description, compte);
		this.codeRecipient = codeRecipient;
	}
	
	
	
	

}
