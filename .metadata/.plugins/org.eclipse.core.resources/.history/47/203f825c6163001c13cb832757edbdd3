package pay.my.buddy.entities;

import java.util.Date;

import javax.persistence.DiscriminatorValue;
import javax.persistence.Entity;

@Entity
@DiscriminatorValue("M")
public class Virement extends Operation{
	private Compte Compte2;

	public Virement() {
		super();
	}

	public Compte getCompte2() {
		return Compte2;
	}

	public void setCodeCompte2(Compte Compte2) {
		this.Compte2 = Compte2;
	}

	public Virement(Date dateOperation, double amount, String description, Compte compte, Compte Compte2) {
		super(dateOperation, amount, description, compte);
		this.Compte2 = Compte2;
	}
	
	
	
	

}
